package day10;

public class Test {
	public static void main(String[] args) {
		int i = 0;
		try {//用try()来括住一段有可能出现异常的代码段
			System.out.println(1/i);
		}catch(Exception e) {
			//当不知道捕获的是什么类型的异常时，可以直接使用所有异常的父类Exception
			e.printStackTrace();
		}
		System.out.println("OK");
	}
}

class A{
	int i;
}
