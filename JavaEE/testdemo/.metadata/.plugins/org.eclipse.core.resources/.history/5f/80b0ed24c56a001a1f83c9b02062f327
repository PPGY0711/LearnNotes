package day09;

public class Person {
	String name;
    int age = 1;
	public Person() {
		this.name = "zhangsan";
		System.out.println("Person constructor");
		System.out.println(this.name);
	}
	
	//非静态的代码块
	{
		System.out.println("code 1");
		System.out.println("---------");
	}
	
	//非静态的代码块
	{
		System.out.println("code 2");
		System.out.println("---------");
	}
	
	//非静态的代码块
	{
		System.out.println("code 3");
		System.out.println("---------");
	}
}
