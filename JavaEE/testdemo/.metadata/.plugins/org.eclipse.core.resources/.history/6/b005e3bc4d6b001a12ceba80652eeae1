package day11;

public class Test3 {

}

enum Season{
	SPRING("春天","春暖花开"),//要调用Season的构造方法
	SUMMER("夏天","烈日炎炎"),
	AUTUMN("秋天","硕果累累"),
	WINTER("冬天","寒冷刺骨");
	
	private final String name;
	private final String desc;//描述
	
	private Season(String name, String desc) {
		this.name = name;
		this.desc = desc;
	}
}