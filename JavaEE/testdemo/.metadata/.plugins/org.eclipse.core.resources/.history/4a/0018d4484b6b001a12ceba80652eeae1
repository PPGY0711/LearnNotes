public class Test2 {
	public static void main(String[] args) {
		Dd d = new Dd();
		List<String> l1 = new ArrayList<String>();
		d.test(l1);
		List<Integer> l2 = new ArrayList<Integer>();
		d.test(l2);
		
		List<C1> 1c = new ArrayList<C1>
	}
}

